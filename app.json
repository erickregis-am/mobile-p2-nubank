{
  "expo": {
    "name": "mobile-p2",
    "slug": "mobile-p2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/nubankIcon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/nubankIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#810AD0"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/nubankIcon.png",
        "backgroundColor": "#810AD0"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Graphik-Black-Trial.otf",
            "./assets/fonts/Graphik-BlackItalic-Trial.otf",
            "./assets/fonts/Graphik-Bold-Trial.otf",
            "./assets/fonts/Graphik-BoldItalic-Trial.otf",
            "./assets/fonts/Graphik-Extralight-Trial.otf",
            "./assets/fonts/Graphik-ExtralightItalic-Trial.otf",
            "./assets/fonts/Graphik-Light-Trial.otf",
            "./assets/fonts/Graphik-LightItalic-Trial.otf",
            "./assets/fonts/Graphik-Medium-Trial.otf",
            "./assets/fonts/Graphik-MediumItalic-Trial.otf",
            "./assets/fonts/Graphik-Regular-Trial.otf",
            "./assets/fonts/Graphik-RegularItalic-Trial.otf",
            "./assets/fonts/Graphik-Semibold-Trial.otf",
            "./assets/fonts/Graphik-SemiboldItalic-Trial.otf"
          ]
        }
      ],
      "expo-router"
    ]
  }
}
